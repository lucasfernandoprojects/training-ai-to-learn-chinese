void startLearningMenu() {
  currentState = LEARNING_MENU;
  learnSelection = 0;
  firstVisibleItem = 0;
  lastVisibleItem = MAX_VISIBLE_ITEMS - 1;
  digitalWrite(RED_LED, LOW);
  digitalWrite(GREEN_LED, LOW);
  showLearningMenu();
}

void showLearningMenu() {
  currentState = LEARNING_MENU;
  
  tft.fillScreen(ST77XX_BLACK);
  tft.setTextColor(ST77XX_WHITE);
  tft.setTextSize(2);
  tft.setCursor(10, 20);
  tft.println("Characters");

  // Adjust visible items based on selection
  if (learnSelection < firstVisibleItem) {
    firstVisibleItem = learnSelection;
    lastVisibleItem = firstVisibleItem + MAX_VISIBLE_ITEMS - 1;
  } else if (learnSelection > lastVisibleItem) {
    lastVisibleItem = learnSelection;
    firstVisibleItem = lastVisibleItem - MAX_VISIBLE_ITEMS + 1;
  }

  // Ensure we don't go out of bounds
  lastVisibleItem = min(lastVisibleItem, charactersCount);
  firstVisibleItem = max(0, firstVisibleItem);

  // Display visible character list
  for (int i = firstVisibleItem; i <= lastVisibleItem; i++) {
    int displayIndex = i - firstVisibleItem;
    int yPos = MENU_TOP_POS + displayIndex * ITEM_HEIGHT;
    
    tft.setCursor(0, yPos);
    if (i == learnSelection) {
      tft.setTextColor(ST77XX_RED);
    } else {
      tft.setTextColor(ST77XX_WHITE);
    }
    
    // Print item number
    tft.print(i + 1);
    tft.print(". ");
    
    // Print character pinyin or "Go back"
    if (i < charactersCount) {
      tft.println(characters[i].pinyin);
    } else {
      tft.println("Go back");
    }
  }
}

void showCharacterView() {
  currentState = CHARACTER_VIEW;
  const CharacterInfo& character = characters[learnSelection];
  
  tft.fillScreen(ST77XX_BLACK);
  
  // Display character bitmap (centered)
  int charX = (tft.width() - character.width) / 2;
  int charY = 20;
  tft.drawBitmap(charX, charY, character.bitmap, character.width, character.height, ST77XX_WHITE);
  
  // Display instruction
  tft.setTextSize(1);
  tft.setTextColor(ST77XX_WHITE);
  tft.setCursor(0, tft.height() - 20);
  tft.println("Press ENTER to return");
}

