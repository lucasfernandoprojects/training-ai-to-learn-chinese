// Menu system
enum SystemState {MAIN_MENU, WAITING_PHOTO, SHOWING_RESULT, LEARNING_MENU, CHARACTER_VIEW, ABOUT_SCREEN};
SystemState currentState = MAIN_MENU;
unsigned long resultDisplayTime = 0;

// Menu items
const char* menuItems[] = {"Inference", "Learn", "About"};
const int menuSize = 3;
int currentSelection = 0;

// Learning mode variables
const int charactersCount = sizeof(characters) / sizeof(characters[0]);
int learnSelection = 0;
int firstVisibleItem = 0;    // Index of first visible item
int lastVisibleItem = MAX_VISIBLE_ITEMS - 1; // Index of last visible item

void setup() {
  pinMode(GREEN_LED, OUTPUT);
  pinMode(RED_LED, OUTPUT);
  pinMode(BUTTON_PIN, INPUT_PULLUP);
  pinMode(UP_BUTTON, INPUT_PULLUP);
  pinMode(DOWN_BUTTON, INPUT_PULLUP);
  pinMode(TFT_BL, OUTPUT);
  digitalWrite(TFT_BL, HIGH);

  tft.init(135, 240);
  tft.setRotation(2);
  tft.fillScreen(ST77XX_BLACK);
  
  Serial.begin(9600);
  
  showWelcomeScreen();
  delay(2000);
  showMainMenu();
}

void loop() {
  switch(currentState) {
    case MAIN_MENU:
      handleMainMenu();
      break;
    case WAITING_PHOTO:
      handleWaitingPhoto();
      break;
    case SHOWING_RESULT:
      handleShowingResult();
      break;
    case LEARNING_MENU:
      handleLearningMenu();
      break;
    case CHARACTER_VIEW:
      handleCharacterView();
      break;
    case ABOUT_SCREEN:
      handleAboutScreen();
      break;
  }
}

void handleMainMenu() {
  // Up button navigation
  if (buttonPressed(UP_BUTTON)) {
    currentSelection = max(currentSelection - 1, 0);
    showMainMenu();
    delay(150); // Debounce delay
  }
  
  // Down button navigation
  if (buttonPressed(DOWN_BUTTON)) {
    currentSelection = min(currentSelection + 1, menuSize - 1);
    showMainMenu();
    delay(150); // Debounce delay
  }

  // Main button press
  if (buttonPressed(BUTTON_PIN)) {
    if (menuItems[currentSelection] == "Inference") {
      startInferenceMode();
    } else if (menuItems[currentSelection] == "Learn") {
      startLearningMenu();
    } else if (menuItems[currentSelection] == "About") {
      showAboutScreen();
    }
  }
}

void handleLearningMenu() {
  // Up button navigation
  if (buttonPressed(UP_BUTTON)) {
    learnSelection = max(learnSelection - 1, 0);
    showLearningMenu();
    delay(150); // Debounce delay
  }
  
  // Down button navigation
  if (buttonPressed(DOWN_BUTTON)) {
    learnSelection = min(learnSelection + 1, charactersCount);
    showLearningMenu();
    delay(150); // Debounce delay
  }

  // Main button press
  if (buttonPressed(BUTTON_PIN)) {
    if (learnSelection == charactersCount) {
      returnToMainMenu();
    } else {
      showCharacterView();
    }
  }
}

void handleCharacterView() {
  // Button press to return to learning menu
  if (buttonPressed(BUTTON_PIN)) {
    showLearningMenu();
  }
}

void handleAboutScreen() {
  // Button press to return to main menu
  if (buttonPressed(BUTTON_PIN)) {
    showMainMenu();
  }
}

void handleWaitingPhoto() {
  // Check for return to main menu (DOWN_BUTTON)
  if (buttonPressed(DOWN_BUTTON)) {
    returnToMainMenu();
    return;
  }

  // Main button press for capture
  if (buttonPressed(BUTTON_PIN)) {
    Serial.println("CAPTURE");
    digitalWrite(RED_LED, LOW);
  }

  if (Serial.available()) {
    String result = Serial.readStringUntil('\n');
    result.trim();
    if (result.startsWith("RESULT:")) {
      showResultScreen(result.substring(7));
    }
  }
}

void handleShowingResult() {
  // Automatically return to photo mode after 5 seconds
  if (millis() - resultDisplayTime >= 5000) {
    returnToPhotoMode();
  }
}

bool buttonPressed(int pin) {
  static unsigned long lastPress = 0;
  if (digitalRead(pin) == LOW && millis() - lastPress > 300) {
    lastPress = millis();
    return true;
  }
  return false;
}

