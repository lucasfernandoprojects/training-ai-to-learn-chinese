void showResultScreen(String className) {
  Serial.print("Processing result for: ");
  Serial.println(className);
  
  currentState = SHOWING_RESULT;
  resultDisplayTime = millis();
  digitalWrite(RED_LED, LOW);
  digitalWrite(GREEN_LED, HIGH);
  
  // Find character info
  const CharacterInfo* charInfo = nullptr;
  for (const auto& c : characters) {
    if (className.equals(c.name)) {
      charInfo = &c;
      break;
    }
  }

  if (!charInfo) {
    Serial.println("Character not found in database!");
    return;
  }

  tft.fillScreen(ST77XX_BLACK);
  
  // Display character bitmap
  int charX = (tft.width() - charInfo->width) / 2;
  int charY = 10;
  tft.drawBitmap(charX, charY, charInfo->bitmap, charInfo->width, charInfo->height, ST77XX_WHITE);
  
  // Display auto-return message
  tft.setTextSize(1);
  tft.setCursor(0, 230);
  tft.println("Loading photo mode...");
}

